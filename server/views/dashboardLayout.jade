  body
    // begin #page-loader
    #page-loader
      .material-loader
        svg.circular(viewbox='25 25 50 50')
          circle.path(cx='50', cy='50', r='20', fill='none', stroke-width='2', stroke-miterlimit='10')
        .message Loading...
    // end #page-loader
    // begin #page-container
    #page-container.fade.page-sidebar-fixed.page-header-fixed.page-with-wide-sidebar
      // begin #header
      #header.header.navbar.navbar-default.navbar-fixed-top
        // begin container-fluid
        .container-fluid
          // begin mobile sidebar expand / collapse button
          .navbar-header
            button.navbar-toggle.collapsed.navbar-toggle-left(type='button', data-click='sidebar-minify')
              span.icon-bar
              span.icon-bar
              span.icon-bar
            button.navbar-toggle(type='button', data-click='sidebar-toggled')
              span.icon-bar
              span.icon-bar
              span.icon-bar
            a.navbar-brand(href='index.html')
              | Awesome B2C Campagin Dashboard
          // end mobile sidebar expand / collapse button
          // begin header navigation right
          ul.nav.navbar-nav.navbar-right
            li
              a.icon.notification.waves-effect.waves-light(href='#', data-toggle='navbar-search')
                i.material-icons search
            li.dropdown
            li
              a#btn-login(href='') LOGIN
            li.dropdown.navbar-user.hidden.btn-userprofile
              a.dropdown-toggle(href='javascript:;', data-toggle='dropdown')
                img.img-profile(src='../dashboard/assets/img/user.jpg', alt='')
                span.hidden-xs xxxx
              ul.dropdown-menu.animated.fadeInLeft
                li.arrow
                li
                  a(href='/profile') View Profile
                li
                  a(href='/settings') Settings
                li.divider
                li
                  a#btn-logout(href='/logout') Log Out
          // end header navigation right
          .search-form
            button.search-btn(type='submit')
              i.material-icons search
            input.form-control(type='text', placeholder='Search Something...')
            a.close(href='#', data-dismiss='navbar-search')
              i.material-icons close
        // end container-fluid
      // end #header
      // begin #sidebar
      #sidebar.sidebar
        // begin sidebar scrollbar
        div(data-scrollbar='true', data-height='100%')
          // begin sidebar user
          ul.nav
            li.nav-profile.hidden.btn-userprofile
              a(href='#', data-toggle='nav-profile')
                .image
                  img.img-profile(src='assets/img/user.jpg', alt='')
                .info
                  b.caret.pull-right
                  |                                 John Smith
            li
              ul.nav.nav-profile
                li
                  a(href='#')
                    i.material-icons settings
                    |  Settings
                li
                  a(href='#')
                    i.material-icons mode_edit
                    |  Send Feedback
                li
                  a(href='#')
                    i.material-icons help
                    |  Helps
          // end sidebar user
          // begin sidebar nav
          ul.nav
            li.nav-header USER MANAGEMENT
            li.has-sub.active
              a(href='javascript:;')
                // <b class="caret pull-right"></b>
                i.material-icons home
                span Dashboard
              //
                <ul class="sub-menu">
                <li class="active"><a href="index.html">Dashboard v1</a></li>
                <li><a href="index_v2.html">Dashboard v2</a></li>
                </ul>
            li.has-sub
              a(href='javascript:;')
                b.caret.pull-right
                i.material-icons inbox
                span Users
              ul.sub-menu
                li
                  a(href='#') User Search
                li
                  a(href='#') User Activity (Auth0 Logs)
                li
                  a(href='#') User Profile
            li.has-sub
